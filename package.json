{
  "main": "build/electron/main.js",
  "homepage": ".",
  "description": "print fw hix",
  "author": "JSH",
  "nodeGypRebuild": true,
  "name": "w10",
  "version": "0.1.0",
  "private": true,
  "build": {
    "extends": null,
    "directories": {
      "buildResources": "assets"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "armv7l",
            "x64"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-circular-progressbar": "^1.1.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-modal": "^3.13.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-scroll": "^1.8.3",
    "@types/styled-components": "^5.1.25",
    "class-names": "^1.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "js-base64": "^3.7.2",
    "node-addon-api": "^5.0.0",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.15.1",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.8.7",
    "sass": "^1.55.0",
    "styled-components": "^5.3.6",
    "ts-stopwatch": "^0.0.4",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "electron:worker:build": "tsc --build electron/worker",
    "electron:dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:dev:nosandbox": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w --no-sandbox\" \"wait-on http://localhost:3000 && tsc -p electron && electron . --no-sandbox\"",
    "electron:build": "rm -rf ./build-addon && yarn addon:build && mv ./build ./build-addon && yarn build && tsc -p electron && electron-builder build",
    "electron:build:rpi": "rm -rf ./build-addon && yarn addon:arm && mv ./build ./build-addon && yarn build && tsc -p electron && electron-builder build",
    "electron:debug": "tsc -p electron",
    "electron-start-win": "set ELECTRON_START_URL=http://localhost:3000 && tsc -p electron     && electron .",
    "electron-start-win-watch": "set ELECTRON_START_URL=http://localhost:3000 && tsc -p electron -w ",
    "react-start-win": "set BROWSER=none && yarn start",
    "addon:init": "node-gyp configure",
    "addon:arm": "cross-env AR=arm-linux-gnueabihf-ar LINK=arm-linux-gnueabihf-g++ CC=arm-linux-gnueabihf-gcc CXX=arm-linux-gnueabihf-g++ node-gyp configure --arch=arm build --release --build-dir=build-addon",
    "addon:clean": "node-gyp clean",
    "addon:build": "node-gyp clean configure build --release",
    "addon:build:dev": "node-gyp clean configure build --debug"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "node-gyp": "^9.1.0",
    "sass-loader": "^12.6.0",
    "wait-on": "^6.0.1"
  }
}
